{
  "name": "post-processing",
  "version": "1.0.0",
  "description": "Transpile legacy Khan Academy code to modern p5.js",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint:eslint": "eslint .",
    "lint:cspell": "cspell lint \"**\"",
    "lint": "concurrently npm:lint:eslint npm:lint:cspell",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest",
    "dev": "concurrently -ikr --default-input-target 1 npm:watch nodemon",
    "prepare": "husky install"
  },
  "author": "Bhavjit Chauhan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/generator": "^7.18.2",
    "@babel/parser": "^7.18.4",
    "@babel/traverse": "^7.18.2",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^7.2.2",
    "cspell": "^6.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.5",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@babel/types": "^7.18.4",
    "ajv": "^8.11.0",
    "lebab": "^3.1.1"
  }
}
